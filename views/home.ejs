<h1>Latest Blogs</h1>
<input type="text" id="search" placeholder="Search blogs by title...">
<div id="blog-list">
    <% blogs.forEach(blog => { %>
        <div class="blog">
            <h2><a href="/<%= blog.slug %>"><%= blog.title %></a></h2>
            <p><%= blog.description %></p>
        </div>
    <% }) %>
</div>
